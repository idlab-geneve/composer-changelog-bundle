{
    "name": "idlab/composer-changelog-bundle",
    "description": "Composer changelog generator",
    "type": "symfony-bundle",
    "version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Vetterli",
            "email": "michael@idlab.ch"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/console": "^5.4|^6.0",
        "davidrjonas/composer-lock-diff": "^1.7"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Idlab\\ComposerChangelogBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Idlab\\ComposerChangelogBundle\\Tests\\": "tests/"
        }
    }
}
